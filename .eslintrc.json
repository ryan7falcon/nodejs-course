{
  "env": {
    "browser": false,
    "commonjs": false,
    "es2022": true,
    "jest/globals": true
  },
  "extends": [
    "airbnb-base"
  ],
  "parserOptions": {
    "ecmaVersion": 13
  },
  "plugins": [
    "jest"
  ],
  "rules": {
    "semi": "off",
    "no-console": "off",
    "import/extensions": "off",
    "no-unused-vars": "off",
    "no-underscore-dangle": "off",
    "sort-imports": "off"
  },
  "root": true
}