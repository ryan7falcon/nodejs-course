{
    "env": {
        "browser": true,
        "commonjs": false,
        "es2021": true,
        "jest/globals": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "plugins": ["jest"],
    "rules": {
        "semi": "off",
        "no-console": "off",
        "import/extensions": "off"
    },
   "root": true
}
